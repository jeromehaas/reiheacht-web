<a class="service-tile {% if class is defined %} {{ class }} {% endif %}" href="{{ href }}">
	<img class="service-tile__image" src="{{ image }}" alt="Image">
	<div class="service-tile__label label">
  	{% include "atoms/heading" with { class: "label__title", type: "h4", text: heading, color: "white"} %}
		<i class="label__icon icon-double-arrow icon"></i>
	</div>
</a>